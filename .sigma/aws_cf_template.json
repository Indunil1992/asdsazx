            "Arn"
          ]
        },
        "CliCommandCreate": {
          "Fn::Join": [
            "",
            [
              "aws events enable-rule --name custom-rule --event-bus-name my-event-bus-udith\naws events put-targets --rule custom-rule --event-bus-name my-event-bus-udith --targets \"Id=cwecustomRuleTarget,Arn=",
              {
                "Fn::GetAtt": [
                  "asdsazx",
                  "Arn"
                ]
              },
              "\""
            ]
          ]
        },
        "CliCommandDelete": "aws events remove-targets --rule custom-rule --event-bus-name my-event-bus-udith --ids cwecustomRuleTarget",
        "CliCommandUpdate": {
          "Fn::Join": [
            "",
            [
              "aws events enable-rule --name custom-rule --event-bus-name my-event-bus-udith\naws events put-targets --rule custom-rule --event-bus-name my-event-bus-udith --targets \"Id=cwecustomRuleTarget,Arn=",
              {
                "Fn::GetAtt": [
                  "asdsazx",
                  "Arn"
                ]
              },
              "\""
            ]
          ]
        }
      }
    },
